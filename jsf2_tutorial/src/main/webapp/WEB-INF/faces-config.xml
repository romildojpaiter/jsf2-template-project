<?xml version="1.0" encoding="UTF-8"?>

<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://www.oracle.com/webfolder/technetwork/jsc/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <lifecycle>	
		<phase-listener>br.com.portalcom.core.utils.listener.LogPhaseListener</phase-listener>
	</lifecycle>

	<application>
		<!-- configuracoes do locale (i18n) -->
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config>

		<resource-bundle>
			<base-name>messages</base-name>
			<var>msg</var>
		</resource-bundle>
		
	</application> 
	
	
